{
    "name": "muffin/sti",
    "description": "Single Table Inheritance for CakePHP 3 ORM",
    "type": "cakephp-plugin",
    "keywords": [
        "cakephp",
        "muffin",
        "sti"
    ],
    "homepage": "https://github.com/usemuffin/sti",
    "license": "MIT",
    "authors": [
        {
            "name": "Jad Bitar",
            "homepage": "http://jadb.io",
            "role": "Author"
        },
        {
            "name": "ADmad",
            "homepage": "https://github.com/ADmad",
            "role": "Author"
        },
        {
            "name": "Others",
            "homepage": "https://github.com/usemuffin/sti/graphs/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/usemuffin/sti/issues",
        "source": "https://github.com/usemuffin/sti"
    },
    "require": {
        "cakephp/orm": "^3.5"
    },
    "require-dev": {
        "cakephp/chronos": "^1.1",
        "cakephp/cakephp": "^3.5",
        "cakephp/cakephp-codesniffer": "^3.0",
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Muffin\\Sti\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Muffin\\Sti\\Test\\": "tests",
            "Muffin\\Sti\\TestApp\\": "tests/test_app"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
